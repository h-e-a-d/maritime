# AIS Stream API Key
# Get your API key from https://aisstream.io/
AIS_API_KEY=your_api_key_here

# Server Port (Railway will set this automatically)
PORT=3000

# MMSI Range Filtering (Optional)
# Filter ships by MMSI range - only ships within this range will be forwarded to clients
# Leave blank to receive all ships
# Example: Portuguese vessels (MID 247) = 247000000 to 247999999
MMSI_MIN=247187700
MMSI_MAX=247435300
